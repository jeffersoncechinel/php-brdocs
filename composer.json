{
  "name": "jeffersoncechinel/php-brdocs",
  "description": "PHP BrDocs auxilia na validação e formatação de documentos brasileiros como CPF e CNPJ",
  "type": "library",
  "readme": "https://github.com/jeffersoncechinel/php-brdocs/blob/master/README.md",
  "keywords": [
    "cpf",
    "cnpj",
    "validar cpf",
    "validar cnpj",
    "formatar cpf",
    "formatar cnpj",
    "cpf validator",
    "cnpj validator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jefferson Cechinel",
      "email": "jclorets@hotmail.com",
      "homepage": "http://www.jeffersoncechinel.com",
      "role": "Creator"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "require": {
    "php": ">=5.5",
    "ext-mbstring": "*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "JC\\BrDocs\\": "src/",
      "Tests\\": "tests/"
    }
  }
}
